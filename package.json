{
  "name": "guidde-backend",
  "version": "0.0.1",
  "description": "Guidde Backend",
  "main": "index.js",
  "scripts": {
    "test": "npm run build &&  npm run undo-migration:test && npm run migrate:test &&  nyc --reporter=text --reporter=html --require @babel/polyfill --require @babel/register mocha ./tests/*  --timeout 200000 --exit",
    "dev": "npm run build &&  nodemon --exec babel-node src/index.js",
    "migrate": "sequelize db:migrate",
    "migrate:test": " sequelize db:migrate",
    "undo-migration": "sequelize db:migrate:undo:all",
    "undo-migration:test": " sequelize db:migrate:undo:all",
    "build": "babel src -d dist && npm run buildNonJs",
    "start": "node dist/index.js",
    "buildNonJs": "babel-node src/helpers/buildNonJsFiles buildNonJs"
  },
